{
    "Version": "2012-10-17",
    "Statement": [
        {
            "Sid": "AllowLambdaToWrite",
            "Effect": "Allow",
            "Principal": {
                "Service": "lambda.amazonaws.com"
            },
            "Action": [
                "s3:PutObject",
                "s3:PutObjectAcl"
            ],
            "Resource": [
                "arn:aws:s3:::NOME_DO_BUCKET/*"
            ]
        }
    ]
}
